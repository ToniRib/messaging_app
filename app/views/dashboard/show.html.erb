<div class="container text-center">
  <div class="row">
    <div class="col-sm-12">
      <h2 class="dashboard-welcome-msg">Welcome, <%= current_user.first_name %></h2>
      <hr>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-12">
      <h4 class="dashboard-prompt">Who do you want to chat with?</h4>
    </div>
  </div>
  <div class="row users-list">
    <div class="col-sm-12">
      <% @users.each do |user| %>
        <%= render partial: "user", locals: { user: user } %>
      <% end %>
    </div>
  </div>
</div>
